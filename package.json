    "jsencrypt": "^3.3.2",
    "lodash-es": "~4.17.21",
    "markdown-it": "^14.1.0",
    "mitt": "^3.0.0",
    "qs": "^6.11.2",
    "sass": "^1.84.0",
    "vue": "^3.5.15",
    "vue-baidu-map-3x": "^1.0.35",
    "vue-diff": "^1.2.4",
    "vue-echarts": "^6.6.1",
    "vue-router": "4.2.4",
    "xlsx": "^0.18.5",
    "xss": "~1.0.14",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "~17.7.0",
    "@types/diff": "^5.2.3",
    "@types/ip": "^1.1.3",
    "@vitejs/plugin-legacy": "~1.6.2",
    "@vitejs/plugin-vue": "~1.9.3",
    "@vitejs/plugin-vue-jsx": "~1.3.10",
    "commitizen": "~4.3.0",
    "commitlint": "~17.7.1",
    "cz-conventional-changelog": "~3.3.0",
    "eslint": "~8.49.0",
    "eslint-plugin-vue": "~9.18.1",
    "husky": "~8.0.3",
    "lint-staged": "~14.0.1",
    "moment": "^2.30.1",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^2.1.0",
    "prettier": "~3.0.3",
    "rollup-plugin-visualizer": "^5.14.0",
    "screenfull": "^6.0.2",
    "standard-version": "~9.5.0",
    "terser": "^5.37.0",
    "typescript": "~4.6.3",
    "unplugin-vue-define-options": "~1.3.18",
    "vite": "~2.6.3",
    "vite-plugin-compression": "^0.5.1",
    "vue-tsc": "~1.8.5",
    "vue3-seamless-scroll": "^2.0.1"
  },
  "engines": {
    "node": ">=14",
    "npm": ">=6.14",
    "yarn": ">=1.22"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "src/**/*.{vue,js,ts,jsx,tsx,json}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.md": [
      "prettier --write"
    ]
  }
